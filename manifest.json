  "description": "Reads trade data from one tab and inputs it into another.",
  "permissions": ["scripting", "activeTab", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
  },
  "host_permissions": [
    "https://*/*"
  ],
  "icons": {
   "16": "Lamar16.png",
   "32": "Lamar32.png",
   "48": "Lamar48.png",
   "128": "Lamar128.png"
 }
}
